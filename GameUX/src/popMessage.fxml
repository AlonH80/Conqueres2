<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>


<ScrollPane id="pane" fx:id="mPane" prefHeight="202.0" prefWidth="549.0" stylesheets="@conqueresUI.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308">
         <children>
            <Label fx:id="messageLabel" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="527.0" wrapText="true" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="3.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="confirmButton" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#confirmInput" text="Confirm" VBox.vgrow="NEVER" />
         </children>
      </VBox>
   </content>
</ScrollPane>
